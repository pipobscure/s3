{
	"name": "@pipobscure/s3",
	"version": "2.0.9",
	"description": "Simple AWS S3 Library",
	"type": "module",
	"exports": "./dist/s3.js",
	"files": [
		"README.md",
		"package.json",
		"dist/*.js",
		"dist/*.d.ts",
		"!dist/*.test.js",
		"!dist/*.test.d.ts"
	],
	"scripts": {
		"test": "node --test src/*.test.ts",
		"format": "biome format --write src/*.ts *.json",
		"lint": "biome lint --write src/*.ts *.json",
		"build": "tsc --build",
		"watch": "tsc --watch",
		"postversion": "git push --follow-tags"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pipobscure/s3.git"
	},
	"keywords": [
		"AWS",
		"S3"
	],
	"contributors": [],
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "^2.2.6",
		"@types/node": "~24.7.2",
		"typescript": "~5.9.3"
	},
	"dependencies": {
		"xml-js": "~1.6.11"
	}
}
